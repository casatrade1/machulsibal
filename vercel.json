{
  "functions": {
    "api/analyze-product.js": {
      "maxDuration": 30
    },
    "api/generate-content.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
